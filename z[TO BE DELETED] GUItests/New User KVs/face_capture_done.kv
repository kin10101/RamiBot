MDScreen:
    name: 'facecapturedone'
    MDFloatLayout:
        orientation: 'vertical'
        Image:
            source: 'Assets/face_scan_complete.png'
            allow_stretch: True
            keep_ratio: False
            size_hint_y: 1  # Adjust the size of the image
            pos_hint: {'center_x':0.5}
            on_touch_down:
                app.change_screen('mainmenu')

