MDScreen:
    name: 'floor7'

    MDFloatLayout: # main layout


        orientation: 'vertical'

        Image: # Background image
            source: 'Floors KVs/map7.png'
            allow_stretch: True
            keep_ratio: False
            size_hint: 1, 1

        #Back Button
        MDIconButton:
            icon: 'arrow-left'
            pos_hint: {'center_x': 0.1, 'center_y': 0.9}
            user_font_size: "40sp"
            on_press:
                root.manager.current = 'floormaps'
                root.manager.transition.direction = 'right'